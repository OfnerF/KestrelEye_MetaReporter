{
  "pattern": {
    "run": "^run[0-9]+$",
    "test_results": "^(train|val|test)_testresults\\.csv$",
    "metrics": "^metrics\\.csv$",
    "meta": "^meta_.+\\.csv$",
    "train_set": "^train_.*\\.csv$",
    "val_set": "^val_.*\\.csv$",
    "test_set": "^test_.*\\.csv$"
  },
  "look_for": [
